{
  "extends": ["next", "prettier", "next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
  "plugins": ["react", "react-hooks", "prettier", "@typescript-eslint"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "warn",
    "prefer-const": "error",
    "react-hooks/exhaustive-deps": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
